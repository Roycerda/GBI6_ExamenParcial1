$ nano ejercicio_1103.sh 

 #!/bin/bash
 Ikiam=Downloads/CSB-master/unix/data/Saavedra2013/*.txt
 for I in $Ikiam
 do
 Fil=`cat $I | wc -l`
 Col=`head -n 1 $I | tr -d ' ' | tr -d '\n' | wc -c`
 echo $I $Fil $Col
 done

$ bash ejercicio_1103.sh

