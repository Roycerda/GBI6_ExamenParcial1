$ nano ejercicio_1103.sh

 #!/bin/bash
 Ikiam=Downloads/CSB-master/unix/data/Saavedra2013/n15.txt
 for I in $Ikiam
 do
 echo "Name:"
 echo $I
 echo "Filas:"
 cat $I | wc -l
 echo "Columnas:"
 head -n 1 $I | tr -d ' ' | tr -d '\n' | wc -c
 done

$ bash ejercicio_1103.sh
